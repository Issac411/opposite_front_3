{% extends 'base.html.twig' %}

{% block title %}Hello NationController!{% endblock %}

{% block stylesheets %}
    {# 'app' must match the first argument to addEntry() in webpack.config.js #}


{% endblock %}

{% block body %}
    <div class=" container p-3 my-3 text-center md fontA styleBlocCentral">
        <div class="barreBlocElement"><button type="button" class="btn btn-info"><a
            {% if politique is defined  %}
                href="{{ path('effetview', {id:effet.id})}}"
            {% else %}
                href="{{ path('effetviewall')}}"
            {% endif %}

                >Retour</a></button></div>
        <div>
            {{ form_start(form,{'method': 'POST'}) }}

            <div class="form-group">
                {{ form_row(form.Libelle, { 'attr': {'class': 'form-control'} }) }}
            </div>
            <div class="form-group">
                {{ form_row(form.Description, { 'attr': {'class': 'form-control'} }) }}
            </div>
            <div class="input-group mb-3">
                <div class="input-group-prepend">
                    <span class="input-group-text" id="basic-addon3">https://siteImage/icone.png</span>
                </div>
                {{ form_widget(form.icone|raw,  { 'attr': {'class': 'form-control'} }) }}
            </div>
            <div class="form-group">
                {{ form_row(form.TraitElements, { 'attr': {'class': 'form-control'} }) }}
            </div>
            <div class="form-group">
                {{ form_row(form.submit, { 'attr': {'class': 'btn btn-primary'} }) }}
            </div>
            {{ form_end(form) }}
            </div>
        </div>


    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

{% endblock %}


