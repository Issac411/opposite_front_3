{% extends 'base.html.twig' %}

{% block title %}Hello NationController!{% endblock %}

{% block stylesheets %}
    {# 'app' must match the first argument to addEntry() in webpack.config.js #}


{% endblock %}

{% block body %}
<div class="container fontA p-3 my-3 text-center md ">
    {{ form_start(form,{'method': 'POST'}) }}
    <div class=" container p-3 my-3 text-center md ">
        <div class="form-group">
            {{ form_row(form.Libelle, { 'attr': {'class': 'form-control'} }) }}
        </div>
        <div class="form-group">
        {{ form_row(form.Ethnie, { 'attr': {'class': 'form-control selectpicker'} }) }}
        </div>
        <div class="form-group">
            {{ form_row(form.nationPartis, { 'attr': {'class': 'form-control selectpicker'} }) }}
        </div>
        <div class="form-group">
            {{ form_row(form.Effets, { 'attr': {'class': 'form-control selectpicker'} }) }}
        </div>
        <div class="form-group">
            {{ form_row(form.Leader, { 'attr': {'class': 'form-control selectpicker'} }) }}
        </div>
        <div class="input-group mb-3">
            <div class="input-group-prepend">
                <span class="input-group-text" id="basic-addon3">https://siteImage/drapeau.png</span>
            </div>
            {{ form_widget(form.Drapeau|raw,  { 'attr': {'class': 'form-control'} }) }}
        </div>
        <div class="form-group">
            {{ form_row(form.submit, { 'attr': {'class': 'btn btn-primary'} }) }}
        </div>
    </div>
    {{ form_end(form) }}
</div>
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

{% endblock %}


